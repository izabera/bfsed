#!/bin/sed -f

# brainfuck interpreter
# EOF = 0, 8 bit cells, input after !
# input is stored in pattern space, after !
# output is stored in hold space, after !, printed at the end of the program


:doread
$bdoneread
N;bdoread
:doneread

h

# validation
s/!.*//
:a
s/\[[^][]*]//
ta
/[][]/{ s/.*/error/;b }


g

# clean up script
s/!.*//;s/[^].,<>+[-]//g

# z is easy so why not
s/\[[-+]]/z/g


# "precompute" loops == append a counter to each [ or ]

# we'll store the counter after the code
s/$/!/

tdoprec
:doprec
/^[^[]*$/bdoneprec

# find a loop with no subloops, replace with ( count code ) count code ! count+1
/\[\([^][]*\)]\(.*\)!\(.*\)/ s//(\3\1)\3\2!\3X/
tdoprec

:doneprec
s/!.*//

# now [ ] are ( )


# merge program and input again
x
/!/{ s/^[^!]*!//;x;G;s/\n/!/;bmerged }
g;s/$/!/
:merged

# current instruction is 0, last instruction is marked by !
s/.*/?&/


x

# 2^15 empty cells
# this isn't portable but most seds allocate as much memory as needed
s/.*/________/;s/.*/&&&&&&&&/;s/.*/&&&&&&&&/;s/.*/&&&&&&&&/;s/.*/&&&&&&&&!/

# debug
#s/.*/________!/

# current cell is 0
s/_/_!/

x


tmain
:main

# debug
#s/^/code:/p
#s/code://
#x
#s/^/tape:/p
#s/tape://
#x
/^[^?]*?\./b.
/^[^?]*?,/b,
/^[^?]*?+/b+
/^[^?]*?-/b-
/^[^?]*?>/b>
/^[^?]*?</b<
/^[^?]*?(/b(
/^[^?]*?)/b)
/^[^?]*?z/bz
bend


:next
s/?\(.\)/\1?/
bmain


:z
x
s/_x*!/_!/
x
bnext

:+
x
s/!/x!/
# wrap at 8 bits
s/x\{256\}!/!/
x
bnext

:-
x
# ugly
/^[^!]*_!/s/_!/_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx!/
s/x!/!/
x
bnext


# can't move out of bounds
:>
x
s/!_\(x*\)/_\1!/
x
bnext

:<
x
s/_\(x*\)!/!_\1/
x
bnext


# sad label wants you to loop...
:(
x
/x!/ { x;s/?\((X*\)/\1?/;bmain }
x
# must use this shit because of the lack of .*?
s/?(X*/&_/;s/?(\(X*\)_\([^_]*\))\1\([^X]\)/(\1\2)\1?\3/
bmain

# happy label thanks you for looping!
:)
s/^\([^?]*\)(\(X*\)\([^?]*\)?)\2/\1?(\2\3)\2/
bmain

# looping turned out to be relatively straightforward


# , is the dumbest part
:,
/^[^!]*!\x00/{ x;s/_x*!/_!/;x;bnext }
/^[^!]*!$/{ x;s/_x*!/_!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_x!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxxx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxxxx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxxxxx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxxxxxx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxxxxxxx!/;x;bnext }
/^\([^!]*\)!	/{ s//\1!/;x;s/_x*!/_xxxxxxxxx!/;x;bnext }
/^\([^!]*\)!\n/{ s//\1!/;x;s/_x*!/_xxxxxxxxxx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxxxxxxxxxx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxxxxxxxxxxx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxxxxxxxxxxxx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxxxxxxxxxxxxx!/;x;bnext }
/^\([^!]*\)!/{ s//\1!/;x;s/_x*!/_xxxxxxxxxxxxxxx!/;x;bnext }
